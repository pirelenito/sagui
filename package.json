{
  "name": "sagui",
  "version": "8.0.1",
  "description": "Front-end tooling in a single dependency",
  "preferGlobal": false,
  "bin": {
    "sagui": "./bin/sagui.js"
  },
  "main": "index.js",
  "files": [
    ".babelrc",
    ".eslintrc",
    "bin",
    "karma-static-files",
    "flowtype-interfaces",
    "lib",
    "src",
    "template"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore *.spec.js",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint .",
    "test:integration": "mocha --timeout 0 --compilers js:babel-register --recursive --reporter spec --fgrep [integration] ./src",
    "test:unit": "mocha --timeout 0 --compilers js:babel-register --recursive --reporter spec --fgrep [integration] --invert ./src",
    "test:unit:watch": "npm run test:unit -- --watch",
    "prepublish": "not-in-install && npm prune && npm test && npm run build || in-install",
    "postinstall": "node ./bin/sagui.js update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saguijs/sagui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saguijs/sagui/issues"
  },
  "homepage": "https://github.com/saguijs/sagui#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-register": "^6.7.2",
    "chai": "^3.3.0",
    "in-publish": "^2.0.0",
    "mocha": "^3.2.0",
    "temp": "^0.8.3",
    "tmp": "0.0.31"
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-sagui": "6.6.1",
    "chalk": "^1.1.1",
    "clean-webpack-plugin": "^0.1.9",
    "commander": "^2.8.1",
    "css-loader": "^0.26.1",
    "eslint": "~3.7.0",
    "eslint-config-sagui": "^6.0.0",
    "eslint-config-standard": "6.2.0",
    "eslint-config-standard-jsx": "3.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flow-bin": "0.37.4",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.24.1",
    "jasmine-core": "^2.5.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lodash.uniq": "^4.3.0",
    "node-sass": "^4.1.1",
    "null-loader": "^0.1.1",
    "parent-module": "^0.1.0",
    "phantomjs-prebuilt": "^2.1.4",
    "postcss-loader": "^1.2.1",
    "postcss-modules-values": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react-transform-hmr": "^1.0.4",
    "resolve-url-loader": "^1.4.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "template-directory": "^2.0.0",
    "url-loader": "^0.5.6",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.7.3",
    "yaml-loader": "^0.4.0"
  }
}
